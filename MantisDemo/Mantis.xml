<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Mantis Automation Execution" parallel="false">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.ceiwc.compugain.funUtil.CustomReporter" />
	</listeners>
	<test name="Mantis Executionon Chrome">
		<parameter name="sbrowser" value="chrome" />
		<classes>
			<class name="com.compugain.testcases.MantisTestCases">
			</class>
		</classes>
	</test>
	<test name="Mantis Executionon Firefox">
		<parameter name="sbrowser" value="firefox" />
		<classes>
			<class name="com.compugain.testcases.MantisTestCases">
			</class>
		</classes>
		</test>
</suite>
